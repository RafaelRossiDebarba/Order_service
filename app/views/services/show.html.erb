<p style="color: green"><%= notice %></p>

<%= render @service %>

<div class="btn-group">
  <%= link_to '<button class="btn btn-primary">Editar</button>'.html_safe, edit_service_path(@service) %>
  <%= link_to '<button class="btn btn-dark">Voltar</button>'.html_safe, services_path %>

  <%= button_to "Deletar", @service, method: :delete, class: "btn btn-danger" %>
</div>
